{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-http": "*",
        "ext-json": "*",
        "ajaxray/php-watermark": "^0.1.1",
        "artisaninweb/laravel-soap": "0.3.0.9",
        "barryvdh/laravel-dompdf": "^0.8.5",
        "codedredd/laravel-soap": "^1.5",
        "darkaonline/l5-swagger": "^5.8",
        "doctrine/dbal": "^2",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "jenssegers/agent": "^2.6",
        "kyslik/laravel-filterable": "^2.0",
        "laravel/framework": "^5.8",
        "laravel/tinker": "^1.0",
        "laravelcollective/annotations": "^5",
        "setasign/fpdi": "^2.3",
        "strahovka/payment": "1.*",
        "swagger-api/swagger-ui": "3.31.1",
	    "ext-mbstring": "*",
        "ext-zip": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "gitlab-token": {
            "gitlab.com": "CZmkWEzAWrZmPWyVz9r9"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    },
    "repositories": {
        "hooks": {
            "type": "composer",
            "url": "https://larapack.io"
        },
        "local": {
            "type": "vcs",
            "url": "git@gitlab.com:strahovkaru-dev/pkg/str-laravel-payment.git"
        }
    }
}
